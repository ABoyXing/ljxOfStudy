    "import numpy as np\n",
    "n=np.arange(24).reshape(4,6)\n",
    "print(n)\n",
    "# 转置\n",
    "n2=n.T\n",
    "print(n2)\n",
    "\n",
    "# 转置\n",
    "n3=n.transpose()\n",
    "print(n3)"
